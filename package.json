{
  "name": "knx",
  "description": "KNXnet/IP protocol implementation for Node(>=6.x)",
  "version": "2.5.4",
  "engines": {
    "node": ">=16"
  },
  "private": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/ekarak/knx.git"
  },
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "lint": "eslint --ext .ts .",
    "lint-fix": "eslint --fix --ext .ts .",
    "test": "npm run test:conn && npm run test:dptlib && npm run test:proto",
    "test:tape": "node -r esbuild-register ./node_modules/.bin/tape",
    "test:conn": "npm run test:tape -- test/connection/*.ts",
    "test:dptlib": "npm run test:tape -- test/dptlib/*.ts",
    "test:proto": "npm run test:tape -- test/knxproto/*.ts",
    "test:wired": "npm run test:tape -- test/wiredtests/*.ts"
  },
  "main": "./build/index.js",
